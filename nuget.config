<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    <add key="local-packages" value="./generated-dependencies" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget">
      <package pattern="*" />
    </packageSource>
    <packageSource key="local-packages">
      <package pattern="RadixCoreApi.Generated" />
      <package pattern="RadixGatewayApi.Generated" />
    </packageSource>
  </packageSourceMapping>
</configuration>
